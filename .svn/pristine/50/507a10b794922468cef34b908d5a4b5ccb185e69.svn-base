// 权限
export const isPower = (that) => {
    let isPower = true;
    if (sessionStorage.getItem('menuType') !== "2") {
        that.$message({
            type: "warning",
            message: "您没有权限操作",
            center: "true"
        });
        isPower = false;
    }
    return isPower;
}