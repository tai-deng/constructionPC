.flex-between
  display flex
  justify-content space-between

.flex
  display flex

.material-need-container
  padding 18px
  width 100%
  height 100%
  box-sizing border-box
  background-color rgba(245, 246, 251, 1)

  .model-wrapper, .form-wrapper
    width 50%

  .form-wrapper
    background #fff

    .title
      position relative
      background #435089
      text-align center
      color #fff
      font-size 20px
      line-height 40px
      height 40px

      .hiden-material
        position absolute
        right 10px
        top 50%
        transform translateY(-50%)

    .content
      padding 20px

      .teble-wrapper

          tr th
            background #435089

            .cell
              color #fff


.material-structure .el-input__inner, .material-structure .el-textarea__inner
  background #e5e5e5

.material-structure .timeline .ico-wrapper .line
    height: 1px
.material-structure .el-button--success
  background: #4775CA !important
.material-structure .el-button
  padding: 10px 30px

.material-structure .el-button--text
  padding 0
  color #333333
  font-size 18px

.material-structure .el-icon-refresh
  color #435089