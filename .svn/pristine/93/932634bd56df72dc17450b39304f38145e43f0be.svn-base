    async getPeople(){
      //项目组织架构图---树状图
      this.isShowVoB = false
      var text='';
      if(this.company[0].DeptList.length>0){
        this.company[0].DeptList[0].UserRoleList.forEach(element => {
          text+=element.cName+','
        });
        this.chartData.name='';
        this.chartData.children=[];
        this.chartData={
          name:this.company[0].DeptList[0].DepartmentName,
          children:[{
            name:text.slice(0,text.length-1),
            children:[]
          }]
        }
        for(var y=1;y<this.company[0].DeptList.length;y++){
          var ai={name:this.company[0].DeptList[y].DepartmentName,children:[]}
          for(var z=0;z<this.company[0].DeptList[y].UserRoleList.length;z++){
            var list={name:this.company[0].DeptList[y].UserRoleList[z].cName==null?'空':this.company[0].DeptList[y].UserRoleList[z].cName}
            ai.children.push(list);
          }
          this.chartData.children[0].children.push(ai);
        }
        setTimeout(()=>{//动态改变值的时候，必须设置定时传显示（上门的循环会有延时）
          this.isShowVoB=true;
        },500)
        setTimeout(()=>{
          this.showMidden()
        },1500)
      }else{
        console.log('没有树状图数据，显示为空！')
        this.chartData={
          name:'没有数据',
          children:[{ name:'没有数据',children:[]}]
        }
        this.isShowVoB=true;

      }
