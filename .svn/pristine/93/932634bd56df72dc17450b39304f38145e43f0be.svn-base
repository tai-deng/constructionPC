        Name:item.cName,
        Tel:item.cPhone,
        Email:item.Email,
        WeChatNo:item.WeChatNo,
        iSys_Dep_ID:item.iSys_Dep_ID,
        iSys_Company_ID:item.iSys_Company_ID,
        ProjectID:item.ProjectID,
        Building:item.Building,
      }
      if(sessionStorage.getItem('menuType') !== "2"){
        this.$message({type: "warning",message: "您没有权限操作",center: "true"});
        return;
      }
      if(item.cPhone!=''){
        const reg = /^1[3|4|5|7|8][0-9]\d{8}$/;
        if(!reg.test(item.cPhone)){
          this.$message({type: "warning",message: "手机号码格式不对！",center: "true"});
          return;
        }
      }
      if(item.Email!=''){
        const email =/^[A-Za-z0-9\u4e00-\u9fa5]+@[a-zA-Z0-9_-]+(\.[a-zA-Z0-9_-]+)+$/;
        if(!email.test(item.Email)){
          this.$message({type: "warning",message: "邮箱格式不对！",center: "true"});
          return;
        }
      }
      this.addModelLoading=true;
      console.log('this.addModelLoading',this.addModelLoading)
      this.Request(MdfUser(post)).then(res=>{
        if(res.StatusCode=='200'){
          this.jumpCompany(post.iSys_Company_ID);
          this.centerDialogVisible=false;
          this.$message({type: 'success', message: '修改成功！'});
        }else{
          this.$message({type: 'error', message:res.Message});
        }
      })
      this.addModelLoading=false;
    },
    //添加用户的时候
    addUser(item){
      var post={
        CurUserID:item.ID,
        UserName:item.cUserName,
        Name:item.cName,
        Tel:item.cPhone,
        Email:item.Email,
        WeChatNo:item.WeChatNo,
        iSys_Dep_ID:item.iSys_Dep_ID,
        iSys_Company_ID:item.iSys_Company_ID,
        ProjectID:item.ProjectID,
        Building:item.Building,
        UserType:0
      }
      if(post.UserName==''){ 
        this.$message({type: 'error', message:'账号必填！'});
        return false;
      }
      this.addModelLoading=true;
      console.log('this.addModelLoading',this.addModelLoading);
      this.Request(AddUser(post)).then(res=>{
        if(res.StatusCode=='200'){
          this.centerDialogVisible=false;
          if(this.getRoleId.length>0){
            //就表示先添加的角色，添加账号的时候
            post.ID=res.Detiel[0];//RoleId
            post.cRoleName=this.getRoleId[this.getRoleId.length-1];
            this.changeRole(post);
          }else{
            this.jumpCompany(post.iSys_Company_ID);
          }

          this.$message({type: 'success', message: '添加成功！'});
        }else{
          this.$message({type: 'error', message:res.Message});
        }
      })
      this.addModelLoading=false;
    },

    
    

  }
}

</script>
