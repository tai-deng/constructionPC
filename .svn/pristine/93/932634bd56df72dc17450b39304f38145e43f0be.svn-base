        console.log(this.company)
        for(let i=0;i<this.company.length;i++){
          for(let x=0;x<this.company[i].DeptList.length;x++){
            for(let z = 0; z<this.company[i].DeptList[x].UserRoleList.length; z++){
              if(this.company[i].DeptList[x].UserRoleList[z].show){
                this.company[i].DeptList[x].UserRoleList[z].show=false;
              }
            }
          }
        }
        item.show=!item.show;
        this.$forceUpdate();//强制刷新，解决页面不会重新渲染的问题
      },
