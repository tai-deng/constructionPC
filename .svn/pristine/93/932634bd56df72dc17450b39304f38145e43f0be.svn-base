    //个人信息模态框上的确定按钮-cUserName
    submitpeople(){
      if(sessionStorage.getItem('menuType') !== "2"){
        this.$message({type: "warning",message: "您没有权限操作",center: "true"});
        return;
      }
      if(this.people.cPhone!='' ){
        const reg = /^1[3|4|5|7|8][0-9]\d{8}$/;
        if(!reg.test(this.people.cPhone)){
          this.$message({type: "warning",message: "手机号码格式不对！",center: "true"});
          return;
        }
      }
      if(this.people.Email !=''){
        const email =/^[A-Za-z0-9\u4e00-\u9fa5]+@[a-zA-Z0-9_-]+(\.[a-zA-Z0-9_-]+)+$/;
        if(!email.test(this.people.Email)){
          this.$message({type: "warning",message: "邮箱格式不对！",center: "true"});
          return;
        }
      }
      if(this.people.ID=='' || this.people.ID== null){
        //添加新用户 CurUserID
        this.addUser(this.people);
      }else{
        //修改用户
        this.resetpeople(this.people);
      }
    },
