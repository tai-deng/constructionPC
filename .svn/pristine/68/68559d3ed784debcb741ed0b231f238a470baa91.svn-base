            el-col(:span="12" style="padding-right: 0px;width: 440px;margin-left:20px;")
                div(class="grid-content bg-purple-light control-detail new-input-box")
                    div(class="titleBg notice2 text-center")
                        .text-center 管控详情
                    el-form(:inline="true" class="demo-form-inline")
                        div(class="margin-bottom fz14") ①施工方案：
                        div(v-if="constructionPlan.length")
                            div(class="margin-bottom gk-detail" v-for="(item, index) in constructionPlan")
                                el-button(size="mini" class="text-ellipsis scheme-button" :class="{'bgnone':!item.Uploader}" type="primary") {{item.SchemeName}}
                                    input(type="file" @change="uploadFile($event, item.SchemeType, item.AuthenSetId)" :accept="accept" :data-schemeType="item.SchemeType" class="scheme-uplod")
                                //- el-upload(action :on-change="uploadFile" :on-progress="progressFile" :data-schemeType="item.SchemeType" :show-file-list="false" :auto-upload="true")
                                //-     el-button(size="mini" class="text-ellipsis" :class="{'bgnone':!item.Uploader}" type="primary") {{item.SchemeName}}
                                i(class="el-icon-search" :class="{'colnone':!item.Uploader}" @click="see(item.FileUrl)")
                                a(class="el-icon-download" :class="{'colnone':!item.Uploader}" target="_blank" :href="item.FileUrl == '' ? 'javascript:;' : item.FileUrl")
                                i(class="el-icon-delete" :class="{'colnone':!item.Uploader}" @click="deleteScheme(item.ID)") {{(item.ID)}}
                                span(class="scr text-ellipsis") 上传人：{{item.Uploader}}
                                span(class="rzsj text-ellipsis") 认证时间：{{item.ConfirmDate | filterDate}}
                                span(style="display:none" ref="schemeType") {{item.SchemeType}}
                        div(v-else class="not-data") 暂无数据!
