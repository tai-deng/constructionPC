.container-time
  position relative
  padding 18px 0s
  width 100%
  height 100%
  box-sizing border-box
  background-color rgba(245, 246, 251, 1)

  .flex
    >>>
      .el-form-item__content
        display flex

        >div:first-child
          margin-right 40px

  .progress
    position absolute
    bottom 0
    width 100%
    display flex

    .right-wrapper
      width 100%
      margin-top 8px

    .right
      width 100%
      display flex
      align-items center

      .befor-line
        position relative
        margin 20px 28px 0
        height 100%
        padding-top 140px

        .line
          position absolute
          left 0
          top 51%
          width 130px
          border 1px solid #e5e5e5

      .after-title
        position relative
        margin 20px 28px 0
        height 100%
        padding-top 140px

        .line
          position absolute
          left 0
          top 51%
          width 130px
          border 1px solid #e5e5e5

        p
          font-size 24px
          font-weight bold
          line-height 60px

      >>>
        .timeline
          .success
            .timeline-content
              color #67C23A

              .container-time-line
                color #67C23A

          .error
            .timeline-content
              color #F56C6C

              .container-time-line
                color #F56C6C

        .timeline-content
          position relative
          margin-top 20px
          margin-left 30px

          &::after
            content ''
            position absolute
            left -16px
            top -20px
            width 46px
            height 0
            display block
            border-bottom 1px solid
            transform-origin top left
            transform rotate(68deg)

          p
            color #333

        .container-time-line
          width 100px
          height 0
          border-top 2px solid

        .icoClassName
          display block
          width 21px
          height 21px
          background #4775CA
          border-radius 50%

        .container-time-dot
          margin-top 18px
          margin-left -4px
          font-size 30px
          color #409EFF !important

        .timeline-top
          position relative
          left -20px
          width 120px
          background #4775CA
          color #fff
          padding 10px
          border-radius 8px
          line-height 28px
          margin-bottom 20px

          &::after
            content ''
            position absolute
            left 20px
            bottom -12px
            width 0
            height 0
            border-width 12px 16px 0
            border-style solid
            border-color #4775CA transparent transparent /* 灰 透明 透明 */

    .left
      padding 30px
      width 280px
      text-align center
      color #fff
      background #435089
      box-sizing border-box

      p:nth-child(1)
        margin-top 18px

      p:nth-child(2)
        margin-top 30px

      *:nth-child(3)
        margin-top 30px

      p
        font-size 24px
        text-align left
        line-height 40px

    >>>
      .el-carousel__item
        display flex

      .el-carousel__indicators
        display none

      .el-carousel__arrow--left
        display none

      .el-carousel__arrow--right
        right 0
        height 100%
        width 40px
        border-radius inherit
        background #435088

  .top
    position relative
    width 100%
    height 75%
    display flex

    .left, .right
      width 50%
      text-align center

      .model
        height 80%

    .left
      border-right 1px solid #C7C7C7

    .type
      position absolute
      left 10px

      >>>
        .el-select .el-input__inner
          border 1px solid #ecedf1
          border-radius 5px
          overflow hidden

        .el-input__suffix
          right 0
          width 50px
          background #44508a
          border-radius 0 5px 5px 0

        .el-input--suffix
          overflow hidden

        .el-select .el-input .el-select__caret
          color #fff