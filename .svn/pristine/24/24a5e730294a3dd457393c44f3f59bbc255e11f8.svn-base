              </el-col>
            </el-row>
          </el-form-item>
          <el-form-item label="实名卡账号:" prop="name">
            <el-input></el-input>
          </el-form-item>
          <hr>
          <el-form-item label="班组:" prop="region">
            <el-row :gutter="20">
              <el-col :span="7" class="pd-l20">
