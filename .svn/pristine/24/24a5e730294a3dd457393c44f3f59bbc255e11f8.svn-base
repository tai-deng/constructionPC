                </div>
              </el-col>
            </el-row>
          </el-form-item>
          <el-form-item label="安全培训状态:" prop="region">
            <el-row :gutter="20">
              <el-col :span="7" class="pd-l20">
                <div class="grid-content bg-purple">
                  <el-select v-model="ruleForm.region" placeholder="已培训">
                    <el-option label="区域一" value="shanghai"></el-option>
                    <el-option label="区域二" value="beijing"></el-option>
                  </el-select>
                </div>
              </el-col>
            </el-row>
          </el-form-item>
          <hr>
          <el-form-item label="是否有特种作业证:" prop="resource">
            <el-radio-group v-model="ruleForm.resource">
              <el-radio label="有"></el-radio>
              <el-radio label="无"></el-radio>
            </el-radio-group>
          </el-form-item>
          <el-form-item label="特种作业证证号:" prop="name">
            <el-input></el-input>
          </el-form-item>
          <el-form-item label="操作类别:" prop="region">
            <el-row :gutter="20">
              <el-col :span="7" class="pd-l20">
                <div class="grid-content bg-purple">
                  <el-select v-model="ruleForm.region" placeholder="选择类别">
                    <el-option label="区域一" value="shanghai"></el-option>
                    <el-option label="区域二" value="beijing"></el-option>
                  </el-select>
                </div>
              </el-col>
            </el-row>
          </el-form-item>
          <el-form-item label="初次领证日期:" class="new-time">
            <el-col :span="22">
              <el-form-item prop="date1">
                <el-date-picker type="date" placeholder="" v-model="ruleForm.date1" style="width: 100%;"></el-date-picker>
              </el-form-item>
            </el-col>
          </el-form-item>
          <el-form-item label="使用期限:" class="new-time">
            <el-col :span="11">
              <el-form-item prop="date1">
                <el-date-picker type="date" placeholder="" v-model="ruleForm.date1" style="width: 100%;"></el-date-picker>
              </el-form-item>
            </el-col>
            <el-col class="line center" :span="2">至</el-col>
            <el-col :span="11">
              <el-form-item prop="date1">
                <el-date-picker type="date" placeholder="" v-model="ruleForm.date1" style="width: 100%;"></el-date-picker>
              </el-form-item>
            </el-col>
          </el-form-item>
        </el-form>
      </el-form>
    </el-col>
    <span slot="footer" class="dialog-footer">
      <el-button @click="centerDialogVisible = false">取 消</el-button>
      <el-button type="primary" @click="centerDialogVisible = false">确 定</el-button>
    </span>
  </el-dialog>