.container-scope-list
  // height calc(100%-300px) !important
  height 756px
  .button-wrapper
    display flex
    justify-content space-between

  .filter-wrapper
    width 49.5%

    .el-tabs__header
      border-bottom none

  .filter
    margin-top 18px

  .table
    height 632px
    .cell
      font-size 16px
    .el-collapse-item__header
      font-size 16px
    .title
      th
        background #44508a

      th .cell
        color #fff

      .el-table__body-wrapper
        display none

    .content
      .el-collapse-item__content
        padding-bottom 0

      .el-table td
        border-style dashed

      .el-table tr:last-child td
        border none

      .el-table::before
        display none

      .el-collapse-item, .el-collapse-item .el-collapse-item__header
        text-indent 0px
        background #F5F6FB

    tbody tr:last-child td
      border-bottom none

  .text-right
    text-align right

  .text-center
    // text-align center

.new-box-card
  padding-left 20px
  padding-right 20px

.new-box-card .new-list-item
  margin 20px 0
  font-size 20px
  color #606060

.new-box-card .el-card__body
  padding 0

.new-box-card .border-bottom
  padding 20px 0
  border-bottom 1px solid #eee

.new-box-card .el-card__header
  padding 0

.container-box-list .el-input__suffix
  width 0.666667rem
  background #44508a
  right 0
  border-radius 0 5px 5px 0

.container-box-list .el-select .el-input .el-select__caret
  color #fff

.container-box-list .el-button--primary
  background #44508a
  border-color #44508a

.container-box-list .el-pagination.is-background .el-pager li
  background #fff

.container-box-list .el-pagination.is-background .el-pager li:not(.disabled).active
  background #44508a

.container-box-list .el-pagination
  margin-top 20px