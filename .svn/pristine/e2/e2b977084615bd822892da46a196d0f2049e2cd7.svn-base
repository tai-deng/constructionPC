.container-human
  padding 18px
  width 100%
  height 100%
  box-sizing border-box
  background-color rgba(245, 246, 251, 1)

  .primary-style
    background #4775CA
    color #fff

  .primary-default
    background #fff

  .model
    background #4580FF
    color #fff

  .exports
    background #FDA143
    color #fff

  .content
    position relative
    width 100%
    height 850px

    .pagination
      margin 20px auto
      text-align center

    .table
      width 81%
      float left 
      margin-left 1%

      >>>
        thead tr, thead tr th
          background #435089

        thead th div
          text-align center
          color #fff

        tr td .cell
          text-align center

    .menu
      width 18%
      height 100%
      float left

      .tit
        position relative
        background #435089
        text-align center
        color #fff
        font-size 18px
        line-height 40px
        height 40px

        span
          position absolute
          left 20px
          top 50%
          transform translateY(-50%)
      .allgroup
        background-color #ffffff
        height 94%
        line-height 50px
        font-size 18px
      .list
        background-color #ffffff
        height 50px
        line-height 50px
        font-size 18px
        padding-left 25%
        box-sizing border-box
      .del_humen_img
        width 35px
        float right
        margin-right 30px
        cursor pointer

  .right-btns
    position absolute
    right 20px

  .attendance
    >div, >ul
      margin 0 auto

    .tips
      margin-top 28px
      display flex

      .is-required::before
        content '*'
        color #F56C6C
        margin-right 2px

      .dot
        position relative
        padding-left 18px
        margin-right 30px

        &::before
          content ''
          display block
          position absolute
          top 50%
          left 3px
          width 12px
          height 12px
          background #909399
          border-radius 50%
          transform translateY(-50%)

        &.error::before
          background #F56C6C

        &.warning::before
          background #E6A23C

        &.brand::before
          background #409EFF

    .day-style
      position absolute
      top 50%
      width 100%
      text-align center
      font-size 40px
      transform translateY(-50%)

      span
        $h = 12px
        position absolute
        left 50%
        bottom -16px
        margin-left -4px
        width $h
        height $h
        border-radius 50%
        background #909399

        &.error
          background #F56C6C

        &.warning
          background #E6A23C

        &.brand
          background #409EFF

  .dialog
    .info
      .btns
        margin-top 20px
        text-align center

        .edit
          width 160px
          background #4775CA
          color #fff

        .addBlank
          width 160px
          color #fff
          background #4580FF

      .two
        margin-top 18px
        margin-bottom 18px
        border-top 1px solid #E1E2E5
        border-bottom 1px solid #E1E2E5

        li
          font-size 16px
          line-height 44px
          height 44px

      .two, .three
        ul
          display flex
          flex-wrap wrap

          li
            line-height 44px
            height 44px

      .two ul li
        width 25%

      .three ul li
        width 33%

        &:nth-child(7), &:nth-child(8), &:nth-child(9), &:nth-child(10), &:nth-child(11)
          width auto
          margin-right 20px

      .one
        position relative

        img
          position absolute

        ul
          padding-left 160px
          display flex
          flex-wrap wrap

          li
            width 255px
            line-height 44px
            height 44px

            &.addres
              width 100%

            &.card
              width 440px

.new-dialog-form .el-input__suffix
  width 50px
  background #44508a
  border-radius 0 5px 5px 0
  right 0

.new-dialog-form .el-input__inner, .new-dialog-form .el-textarea__inner
  background rgba(248, 248, 248, 1)
  border 1px solid rgba(229, 229, 229, 1)
  border-radius 4px

.new-dialog-form .el-select .el-input__inner
  background #fff

.new-dialog-form .el-select .el-input .el-select__caret
  color #fff

.new-dialog-form .pd-l20
  padding-left 0 !important

.new-dialog-form .demo-ruleForm
  padding 20px

.new-dialog-form .el-form-item__label
  width 22% !important

.new-dialog-form .el-form-item__content
  margin-left 22% !important
  margin-right 3%

.new-dialog-form .new-time .el-input__suffix
  background none

.new-dialog-form .new-time .el-input__prefix
  left inherit
  right 15px

.new-dialog-form .new-time .el-form-item__content .el-form-item__content
  margin-left 0 !important
  margin-right 0

.new-dialog-form .el-button
  width 100px
  height 34px
  padding 0

.new-dialog-form .el-button--primary
  background #4775CA

.new-dialog-form .el-button--default
  background #999999
  color #fff

.new-dialog-form .el-dialog__header, .container-human .el-dialog__header
  background linear-gradient(-90deg, rgba(105, 86, 255, 1), rgba(84, 138, 255, 1))
  line-height 10px

.new-dialog-form .el-dialog__header .el-dialog__title, .container-human .el-dialog__header .el-dialog__title
  color #fff

.new-dialog-form .el-dialog__header .el-dialog__headerbtn .el-dialog__close, .new-dialog-form .el-dialog__header .el-dialog__close
  color #fff

.new-dialog-form .center, .container-human .el-dialog__header
  text-align center

.new-dialog-form .new-upload-img .el-form-item__content
  margin-left 5% !important

.new-dialog-form .new-upload-img .tip
  margin-left: 20px
  margin-bottom: 20px
  // position absolute
  // left 175px
  // bottom 0

  .col999
    color #999

.new-dialog-form .el-select .el-input__inner::placeholder
  color rgba(51, 51, 51, 1)

.new-dialog-form .el-form--inline .el-form-item__content
  width 260px
  height 40px
  background rgba(255, 255, 255, 1)
  box-shadow 0px 1px 7px 0px rgba(216, 223, 238, 0.75)
  border-radius 4px
  font-size 18px
  font-family SourceHanSansCN-Normal
  font-weight 400

.avatar-uploader .el-upload {
  border: 1px dashed #d9d9d9;
  border-radius: 6px;
  cursor: pointer;
  position: relative;
  overflow: hidden;
}

.avatar-uploader .avatar-uploader-icon {
  font-size: 28px;
  color: #8c939d;
  width: 178px;
  height: 178px;
  line-height: 178px;
  text-align: center;
}

.avatar-uploader .avatar
  width: 178px;
  height: auto;

.wrap
  display flex
  flex-direction row
  align-items flex-end