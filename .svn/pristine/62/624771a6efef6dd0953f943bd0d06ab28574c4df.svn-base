        //合并模态框上面的选择模型的时候---记录名字（用于排序显示) selectMedelName
        saveSelectModel(){
            for(var i=0;i<this.model.length;i++){
                if(this.selectMedel==this.model[i].value){
                    this.selectMedelName=this.model[i].label
                }
            }

        },
        //合并模态框上的点击添加信息
        addSelectModel:function(){
            if(this.selectOption=='' || this.selectMedel==''){
                return false;
            }
            var indexMax=this.arr.length-1;
            var text={index:indexMax,single:this.selectOption,building:this.selectMedel,singleName:this.selectoptionsName,buildingName:this.selectMedelName};
            this.arr.push(text);
        },
        //加载页面的时候--查询所有
        getAll(){
            this.load();
            this.Request(GetModelList({projectId:localStorage.getItem('projectid')})).then(res=>{
                if(res.StatusCode === 200){
                    this.lists=res.Result.BuildingList; //获取建筑单体
                    this.baseball=[];
                    for(var y=0;y<res.Result.ModeFileList.length;y++){//场地模型
                            if(res.Result.ModeFileList[y].Category=='0'){
                                var text={ID:'',Thumbnails:'',TransStatus:'', dUpdateTime:'',FileID:'' };
                                text.ID=res.Result.ModeFileList[y].ID;
                                text.Thumbnails=res.Result.ModeFileList[y].Thumbnails;
                                text.TransStatus=res.Result.ModeFileList[y].TransStatus;
                                text.dUpdateTime=res.Result.ModeFileList[y].dUpdateTime.split('T')[0];
                                text.FileID=res.Result.ModeFileList[y].FileID;
                                this.baseball.push(text);
                            }
                    }
                    console.log(this.baseball);
                    for(var i=0;i<this.lists.length;i++){
                        this.lists[i].textNameDis=true;   
                        for(var x=0;x<res.Result.ModeFileList.length;x++){ //建筑单体的模型视图
                            if(res.Result.ModeFileList[x].ModelID==this.lists[i].ModelID && res.Result.ModeFileList[x].Category=='1' ){
                                    this.lists[i].firstImg={FileID:res.Result.ModeFileList[x].FileID,
                                            ID:res.Result.ModeFileList[x].ID,
                                            Thumbnails:res.Result.ModeFileList[x].Thumbnails,
                                            TransStatus:res.Result.ModeFileList[x].TransStatus,
                                            dUpdateTime:res.Result.ModeFileList[x].dUpdateTime.split('T')[0]
                                    };
                            }
                            if(res.Result.ModeFileList[x].ModelID==this.lists[i].ModelID && res.Result.ModeFileList[x].Category=='2' ){
                                    this.lists[i].scondImg={FileID:res.Result.ModeFileList[x].FileID,
                                            ID:res.Result.ModeFileList[x].ID,
                                            Thumbnails:res.Result.ModeFileList[x].Thumbnails,
                                            TransStatus:res.Result.ModeFileList[x].TransStatus,
                                            dUpdateTime:res.Result.ModeFileList[x].dUpdateTime.split('T')[0]
                                    };
                            }
                            if(res.Result.ModeFileList[x].ModelID==this.lists[i].ModelID && res.Result.ModeFileList[x].Category=='3' ){
                                    this.lists[i].threeImg={FileID:res.Result.ModeFileList[x].FileID,
                                            ID:res.Result.ModeFileList[x].ID,
                                            Thumbnails:res.Result.ModeFileList[x].Thumbnails,
                                            TransStatus:res.Result.ModeFileList[x].TransStatus,
                                            dUpdateTime:res.Result.ModeFileList[x].dUpdateTime.split('T')[0]
                                    };
                            }  
                        }
                    }
                    for(var z=0;z<res.Result.ModelList.length;z++){
                        res.Result.ModelList[z].dUpdateTime= res.Result.ModelList[z].dUpdateTime.split('T')[0]
                    }
                    console.log(this.lists);
                    this.tagetherMoudle=res.Result.ModelList
                }else{
                    this.$message({type:"error",message:res.Message,center:"true"});
                }
                this.$toast.clear();
            })
        },
        //删除单个模型 DelModelFile
        delModelSingle(item){
            if(item.TransStatus=='2' || item.TransStatus=='1'){
                //不能删除
                this.$message({type: "error",message:'该模型已经被整合了，无法删除！',center: "true"});
                return false;
            }else{
                this.Request(DelModelFile({fileId:item.fileId})).then(res=>{
                    if(res.StatusCode === 200){
                        this.$message({type:"sucess",message:res.Message,center:"true"});
                    }else{
                        this.$message({type:"error",message:res.Message,center:"true"});
                    }
                })
            }
        },
        //分享链接给别人---模态框-场地模型可以分享-----整合模型没有FileID  hasOwnProperty
        shaveOther(item){
            var text='';
            if(item.hasOwnProperty('FileID')){
                text=item.FileID
            }else{
                text=item.ModelID
            }
            console.log(text)
            this.Request(GetSingleFileViewToken({fileId:text})).then(res=>{
                if(res.StatusCode === 200){
                    var ViewToken=res.Detiel;
                    console.log(ViewToken);
                    this.showShare=true;
                    //this.showModelInput='http://127.0.0.1:9999/#/nav/to/components/showModel?viewToken='+ViewToken[0];
                    this.showModelInput='http://127.0.0.1:9999/#/nav/to/components/watchModel.html?viewToken='+ViewToken[0];
                }else{
                    this.$message({type:"error",message:res.Message,center:"true"});
                }
            })
        },
        //删除整合模型
        delTogetModel(item){
            console.log(item);
            this.Request(DeleteMergeFile({modelId:item.ModelID})).then(res=>{
                if(res.StatusCode === 200){
                    this.$message({type:"sucess",message:res.Message,center:"true"});
                }else{
                    this.$message({type:"error",message:res.Message,center:"true"});
                }
            })
        },

         //根据项目ID和建筑单体ID获取详情---上传单个模型的时候--加载图片
        GetDetailList(n){
            console.log(this.lists);
            this.Request(GetDetailList({projectId:localStorage.getItem('projectid'),buildingId:n})).then(res=>{
                if(res.StatusCode === 200){
                    for(var i=0;i<this.lists.length;i++){
                        for(var x=0;x<res.Detiel.length;x++){
                            if(this.lists[i].ID==res.Detiel[x].BuildingID && res.Detiel[x].Category=='1'){
                                this.lists[i].firstImg.FileID=res.Detiel[x].FileID;
                                this.lists[i].firstImg.ID=res.Detiel[x].ID;
                                this.lists[i].firstImg.Thumbnails=res.Detiel[x].Thumbnails;
                                this.lists[i].firstImg.TransStatus=res.Detiel[x].TransStatus;
                                this.lists[i].firstImg.dUpdateTime=res.Detiel[x].dUpdateTime.split('T')[0];
                            }
                            if(this.lists[i].ID==res.Detiel[x].BuildingID && res.Detiel[x].Category=='2'){
                                this.lists[i].scondImg.FileID=res.Detiel[x].FileID;
                                this.lists[i].scondImg.ID=res.Detiel[x].ID;
                                this.lists[i].scondImg.Thumbnails=res.Detiel[x].Thumbnails;
                                this.lists[i].scondImg.TransStatus=res.Detiel[x].TransStatus;
                                this.lists[i].scondImg.dUpdateTime=res.Detiel[x].dUpdateTime.split('T')[0];
                            }
                            if(this.lists[i].ID==res.Detiel[x].BuildingID && res.Detiel[x].Category=='3'){
                                this.lists[i].threeImg.FileID=res.Detiel[x].FileID;
                                this.lists[i].threeImg.ID=res.Detiel[x].ID;
                                this.lists[i].threeImg.Thumbnails=res.Detiel[x].Thumbnails;
                                this.lists[i].threeImg.TransStatus=res.Detiel[x].TransStatus;
                                this.lists[i].threeImg.dUpdateTime=res.Detiel[x].dUpdateTime.split('T')[0];
                            }
                        }
                        
                    }
                    console.log(this.lists)
                }
            })
        },
