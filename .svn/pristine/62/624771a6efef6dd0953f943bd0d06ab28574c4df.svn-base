        cleartogetherName(){
            this.togetherVisible=false;
        },


        //输入合并模型名称的模态框上---发起合并的按钮 MergeFile
        getTogetherAll(){
            console.log('发起合并'); //{TaskIds:TaskIds},post) sources-集成模型文件列表 config-集成模型配置
            console.log(this.lists);
            var sources=[];
            for(var i=0;i<this.lists.length;i++){
                for(var x=0;x<this.arr.length;x++){
                    var text={fileId:'',specialty:''}
                    if(this.arr[x].building=='0'){
                        text.fileId=this.baseball[0].FileID;
                        text.specialty='场地';
                        sources.push(text)
                    }
                    if(this.arr[x].singleName==this.lists[i].BuildingName){
                        if(this.arr[x].building=='1'){
                            text.fileId=this.lists[i].firstImg.FileID;
                            text.specialty='建筑'
                        }else if(this.arr[x].building=='2'){
                            text.fileId=this.lists[i].scondImg.FileID;
                            text.specialty='结构'
                        }else if(this.arr[x].building=='3'){
                            text.fileId=this.lists[i].threeImg.FileID;
                            text.specialty='机电'
                        }
                        sources.push(text)
                    }
                }
            }
           
            var post={ProjectId:localStorage.getItem('projectid'),name:this.togetherName,
                    sources:sources,
                    config:{loadOnDemand:true,fileType:'rvt'},callback:""}
                     console.log(post)
            this.Request(MergeFile(post)).then(res=>{
                console.log('合并:',res);
            })
            
        },

