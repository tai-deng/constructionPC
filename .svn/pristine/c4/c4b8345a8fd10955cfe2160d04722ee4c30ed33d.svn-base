export const pubInterface = (that, pubInterfaceName, param) => {
    return new Promise((resolve,reject)=>{
        that.load()
        that.Request(pubInterfaceName(param)).then(res=>{
            if(res.StatusCode === 200){
                resolve(res)
            }else{
                reject(new Error(res.Message))
            }
            that.$toast.clear()
        })
    })
}