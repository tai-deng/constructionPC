.container-human
  padding 18px
  width 100%
  height 100%
  box-sizing border-box
  background-color rgba(245, 246, 251, 1)
  
  .primary-style
    background #4775CA
    color #fff

  .primary-default
    background #fff

  .model
    background #4580FF
    color #fff

  .exports
    background #FDA143
    color #fff
  .content
    position relative
    width 100%

    .pagination
      margin 20px auto
      text-align center

    .table
      padding-left 320px

      >>>
        thead tr, thead tr th
          background #435089

        thead th div
          text-align center
          color #fff

        tr td .cell
          text-align center

    .menu
      position absolute
      left 0
      top 0
      width 300px

      .tit
        position relative
        background #435089
        text-align center
        color #fff
        font-size 18px
        line-height 40px
        height 40px

        span
          position absolute
          left 20px
          top 50%
          transform translateY(-50%)

          

  .right-btns
    position absolute
    right 20px

  .dialog
    .attendance
      .tips
        margin-top 28px
        display flex

        .is-required::before
          content '*'
          color #F56C6C
          margin-right 2px

        .dot
          position relative
          padding-left 18px
          margin-right 28px

          &::before
            content ''
            display block
            position absolute
            top 50%
            left 4px
            width 8px
            height 8px
            background #909399
            border-radius 50%
            transform translateY(-50%)

          &.error::before
            background #F56C6C

          &.warning::before
            background #E6A23C

          &.brand::before
            background #409EFF

      .day-style
        position absolute
        top 50%
        width 100%
        text-align center
        font-size 40px
        transform translateY(-50%)

        span
          $h = 8px
          position absolute
          left 50%
          bottom -16px
          margin-left -4px
          width $h
          height $h
          border-radius 50%
          background #909399

          &.error
            background #F56C6C

          &.warning
            background #E6A23C

          &.brand
            background #409EFF

    .info
      .btns
        margin-top 20px
        text-align center
        .edit
          width 160px
          background #4775CA
          color #fff
        .addBlank
          width 160px
          color #fff
          background #4580FF

      .two
        margin-top 18px
        margin-bottom 18px
        border-top 1px solid #E1E2E5
        border-bottom 1px solid #E1E2E5

      .two, .three
        ul
          display flex
          flex-wrap wrap

          li
            height 40px
            line-height 40px

      .two ul li 
        width 25%

      .three ul li
        width 33%

        &:nth-child(7), &:nth-child(8), &:nth-child(9), &:nth-child(10), &:nth-child(11)
          width auto
          margin-right 20px

      .one
        position relative

        img
          position absolute

        ul
          padding-left 160px
          display flex
          flex-wrap wrap

          li
            width 33%
            height 40px
            line-height 40px

            &.addres
              width 100%

.new-dialog-form .el-input__suffix
