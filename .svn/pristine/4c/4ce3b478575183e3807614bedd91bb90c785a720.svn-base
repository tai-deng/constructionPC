.new-dialog-form .el-input__inner, .new-dialog-form .el-textarea__inner
  background:rgba(248,248,248,1);
  border:1px solid rgba(229,229,229,1);
  border-radius:4px;

.new-dialog-form .el-select .el-input__inner
