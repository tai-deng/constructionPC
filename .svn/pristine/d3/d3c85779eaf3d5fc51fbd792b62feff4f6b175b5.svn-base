    },
    // 删除事件还原
    onDeleteItem(v) {
      this.$emit("deleteItem", v);
    },

    // 获取列表
    getData() {
      let data = [];
      this.load();
      this.Request(
        Get_JDGH_List({ ProjectID: localStorage.getItem("projectid") })
      )
        .then(res => {
          this.$toast.clear();
          if (res.StatusCode == 200 && res.Detiel.length > 0) {
            res.Detiel.forEach((el, index) => {
              data.push({
                id: el.TaskId,
                text: el.TaskName,
                type: "task",
                start_date: el.BaseStartTime,
                duration: el.Duration,
                parent: el.TaskParentId,
                progress: el.PercenTage,
                open: false
              });
              // if(el.TaskName=="看看"){
              //   console.log(el)
              //   console.log(res.Detiel[index+1])
              // }
              this.isShowGanttm = true;
            });
            this.tasks = { data, links: [] };
            this.updateTasks = false;
            this.$emit("onIsupdataGt", false);
          } else {
            this.$message({
              type: "error",
              message: e.Message,
              center: "true"
            });
          }
          this.$toast.clear();
        })
        .catch(e => {
          this.$message({ type: "error", message: e.Message, center: "true" });
          this.$toast.clear();
        });
    },

    updataGt() {
      this.updateTasks = true;
    }
  },
  watch: {
    isupdataGt(c, o) {
      console.log(c);
      this.updateTasks = c;
    },
    updateTasks(c, o) {
      if (c) {
        this.getData();
      }
    }
  }
};
</script>

